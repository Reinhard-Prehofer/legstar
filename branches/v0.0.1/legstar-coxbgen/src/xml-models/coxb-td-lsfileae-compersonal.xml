<?xml version='1.0' encoding='UTF-8'?>
<coxb-type name="ComPersonalTypeBinding" type="complex">

	<!-- Name of the JAXB type to be bound. -->
	<jaxb-type-name>ComPersonalType</jaxb-type-name>
	<!-- Package for JAXB type.  -->
	<jaxb-type-package>com.legstar.test.coxb.lsfileae</jaxb-type-package> 
	<!-- Name of the JAXB property. Parent type references this property using this name. -->
	<jaxb-property-name>comPersonal</jaxb-property-name>
	
	<!-- Repeat for each property (order is important). -->
	<coxb-property jaxb-name="comName" type="simple" jaxb-type="java.lang.String" binding-type="CStringBinding" cobol-byteLength="20" cobol-isJustifiedRight="false"/>
	<coxb-property jaxb-name="comAddress" type="simple" jaxb-type="java.lang.String" binding-type="CStringBinding" cobol-byteLength="20" cobol-isJustifiedRight="false"/>
	<coxb-property jaxb-name="comPhone" type="simple" jaxb-type="java.lang.String" binding-type="CStringBinding" cobol-byteLength="8" cobol-isJustifiedRight="false"/>
	  
</coxb-type>
